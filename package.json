{
	"name": "change-case-of-symbol",
	"displayName": "Change case of symbol",
	"description": "Allows to change symbol name's case in all its occurences",
	"version": "1.0.0",
	"publisher": "bragov4ik",
	"repository": {
	  "type": "git",
	  "url": "https://github.com/bragov4ik/change-case-rename-vscode.git"
	},	
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"icon": "https://raw.githubusercontent.com/bragov4ik/change-case-rename-vscode/master/icons/icon.png",
	"activationEvents": [
		"onCommand:changeCaseOfSymbol.camelCase",
		"onCommand:changeCaseOfSymbol.capitalCase",
		"onCommand:changeCaseOfSymbol.constantCase",
		"onCommand:changeCaseOfSymbol.dotCase",
		"onCommand:changeCaseOfSymbol.headerCase",
		"onCommand:changeCaseOfSymbol.noCase",
		"onCommand:changeCaseOfSymbol.paramCase",
		"onCommand:changeCaseOfSymbol.pascalCase",
		"onCommand:changeCaseOfSymbol.pathCase",
		"onCommand:changeCaseOfSymbol.sentenceCase",
		"onCommand:changeCaseOfSymbol.snakeCase"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "changeCaseOfSymbol.camelCase",
				"title": "camel case",
				"category": "Change case of symbol"
			},
			{
				"command": "changeCaseOfSymbol.capitalCase",
				"title": "capital case",
				"category": "Change case of symbol"
			},
			{
				"command": "changeCaseOfSymbol.constantCase",
				"title": "constant case",
				"category": "Change case of symbol"
			},
			{
				"command": "changeCaseOfSymbol.dotCase",
				"title": "dot case",
				"category": "Change case of symbol"
			},
			{
				"command": "changeCaseOfSymbol.headerCase",
				"title": "header case",
				"category": "Change case of symbol"
			},
			{
				"command": "changeCaseOfSymbol.noCase",
				"title": "no case",
				"category": "Change case of symbol"
			},
			{
				"command": "changeCaseOfSymbol.paramCase",
				"title": "param case",
				"category": "Change case of symbol"
			},
			{
				"command": "changeCaseOfSymbol.pascalCase",
				"title": "pascal case",
				"category": "Change case of symbol"
			},
			{
				"command": "changeCaseOfSymbol.pathCase",
				"title": "path case",
				"category": "Change case of symbol"
			},
			{
				"command": "changeCaseOfSymbol.sentenceCase",
				"title": "sentence case",
				"category": "Change case of symbol"
			},
			{
				"command": "changeCaseOfSymbol.snakeCase",
				"title": "snake case",
				"category": "Change case of symbol"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"change-case": "^4.1.2"
	}
}
